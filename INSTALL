# Prepare directories:
mkdir bin build
# Go in the 'build' directory
cd build
# Prepare the makefile
cmake ..
# Run the newly created makefile:
make
# Copy the created binaries in ../bin:
cp leon ../bin/
# Go back at the installation root directory
cd ..
#toy example
./bin/leon -file data/toy.fasta -c
#decompress and check file
./bin/leon -file data/toy.leon -d -test-file

 
